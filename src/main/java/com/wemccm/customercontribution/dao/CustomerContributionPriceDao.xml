<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.wemccm.customercontribution.dao.CustomerContributionPriceDao">

	<insert id="save"
		parameterType="com.wemccm.common.entity.CustomerContributionPrice">
		INSERT INTO customercontributionprice
		(customerContributionId,customerContributionPriceTypeId,originalPrice,revisedPrice)
		VALUES
		(#{customerContributionId},#{customerContributionPriceTypeId},#{originalPrice},#{revisedPrice})
	</insert>

</mapper>