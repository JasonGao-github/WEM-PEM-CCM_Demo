<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wemccm.project.dao.projectDao">
	<!--设置domain类和数据库中表的字段一一对应，注意数据库字段和domain类中的字段名称不致，此处一定要！ -->
	<!-- <resultMap id="BaseResultMap" type="com.wemccm.common.entity.User"> 
		<id column="id" property="id" jdbcType="INTEGER" /> <result column="username" 
		property="username" jdbcType="CHAR" /> <result column="password" property="password" 
		jdbcType="CHAR" /> </resultMap> -->
	<!-- 查询单条记录 -->
	
<!-- resultType="com.wemccm.common.entity.User" -->
	<select id="selectPage"
		
		resultType="com.wemccm.common.entity.Project"
		parameterType="com.wemccm.common.page.FindPageRequestDto">
		SELECT * FROM project
		<where>
			<if test="JemenaWBS!=null and JemenaWBS!=''">
				JemenaWBS = #{JemenaWBS}
			</if>
			<if test="ProjectTitle!=null and ProjectTitle!=''">
				ProjectTitle = #{ProjectTitle}
			</if>
			<if test="ProjectType!=null and ProjectType!=''">
				ProjectType = #{ProjectType}
			</if>
			<if test="JemenaSAPPMOrder!=null and JemenaSAPPMOrder!=''">
				JemenaSAPPMOrder = #{JemenaSAPPMOrder}
			</if>
			<if test="SupplyAddress!=null and SupplyAddress!=''">
				SupplyAddress = #{SupplyAddress}
			</if>
			<if test="ZimfraWBS!=null and ZimfraWBS!=''">
				ZimfraWBS= #{ZimfraWBS}
			</if>
			<if test="InquiryNumber!=null and InquiryNumber!=''">
				InquiryNumber = #{InquiryNumber}
			</if>
		</where>
	</select>
</mapper>